{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
       "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react-hooks/rules-of-hooks": "off", // Checks rules of Hooks
        "react-hooks/exhaustive-deps": "off", // Checks effect dependencies
        "semi": 0,
        "quotes":"off",
        "eol-last":"off",
        "array-bracket-spacing":"off"
       
    }
}
